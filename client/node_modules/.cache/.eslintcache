ories.jsx",["339","340","341","342"],[],"import React from \"react\";\n\nclass Categories extends React.Component {\n  render() {\n    return (\n      <div className=\"widget catagory mb-50\">\n        <h6 className=\"widget-title mb-30\" role=\"button\">\n          Categories\n        </h6>\n\n        <div className=\"catagories-menu\">\n          <ul id=\"menu-content2\" className=\"menu-content collapse show\">\n            {this.props.curCategory !== \"all items\" ? (\n              <li style={{ color: \"#787878\" }}>\n                <a onClick={this.props.getAllItems}>all items</a>\n              </li>\n            ) : (\n              <li>\n                <a style={{ fontWeight: 700 }} onClick={this.props.getAllItems}>\n                  all items\n                </a>\n              </li>\n            )}\n            {this.props.uniqueCategories.map((category) =>\n              this.props.curCategory !== category ? (\n                <li style={{ color: \"#787878\" }} key={category}>\n                  <a onClick={this.props.onFilter}>{category}</a>\n                </li>\n              ) : (\n                <li key={category}>\n                  <a style={{ fontWeight: 700 }} onClick={this.props.onFilter}>\n                    {category}\n                  </a>\n                </li>\n              )\n            )}\n          </ul>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default Categories;\n","/Users/luanakimley/nodejs-projects/ecommerce-web-app/client/src/components/ShopSideBar/ColourFilter.jsx",["343","344","345","346"],[],"import React from \"react\";\nimport { COLOURS } from \"../../config/global_constants\";\n\nclass ColourFilter extends React.Component {\n  render() {\n    return (\n      <div className=\"widget color mb-50\">\n        <p className=\"widget-title2 mb-30\">Colour</p>\n        <div className=\"widget-desc\">\n          <ul className=\"d-flex\">\n            {this.props.uniqueColours.map((colour) =>\n              !th